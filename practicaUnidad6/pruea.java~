 */
public class ejercicio7
{
private int suma(int n){
        int suma=0;
        
        while(n>0){
         int dig2;
            int dig=n%10;
        dig2=dig*dig*dig;
        suma=suma+dig2;
        n=n/10;
    }
    return suma;
}
public String esNarcisista(int n){
    
    String res="";
    if(suma(n)==n)
    res="Es narcisista";
    else 
    res="Normal";
    
    return res;
}
}
