public class ejercicio71
{
    public String Narcisista(int n){
        int suma=0; String res="";
        while(n>0){
            int dig2;
            int dig=n%10;
            dig2=dig*dig*dig;
            suma=suma+dig2;
            n=n/10;
            if(suma==n)
            res=" si es";
            else
            res="nop";
        
        return res;
    
}
}